    "antd-mobile": "^5.39.0",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash-es": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-easy-crop": "^5.5.2",
    "react-hook-form": "^7.51.3",
