    "deploy:dev:only": "rm -rf di-dist && yarn build:dev && di-cli uploadDir --branch=dev",
    "deploy": "di-cli uploadDir --branch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "updateIconFont": "node ./script/updateIconFont.cjs"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@di-fe/di-cli": "^1.1.6",
    "ahooks": "^3.7.8",
